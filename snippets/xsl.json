{
  "Dump XPath node": {
    "prefix": "dump",
    "body": [
      "<xsl:copy-of select=\"$1\">",
      "  <xsl:value-of select=\".\" />",
      "</xsl:copy-of>"
    ],
    "description": "Dump an xsl node tree structure"
  },
  "Comment": {
    "prefix": "cm",
    "body": [
      "<!-- $1 -->"
    ],
    "description": "XSL Comment tag"
  },
  "Variable": {
    "prefix": "var",
    "body": [
      "<xsl:variable name=\"$1\">",
      "  $2",
      "</xsl:variable>"
    ],
    "description": "XSL Variable"
  },
  "Choose": {
    "prefix": "choose",
    "body": [
      "<xsl:choose>",
      "  <xsl:when test=\"$1\">",
      "    $2",
      "  </xsl:when>",
      "  <xsl:otherwise>",
      "      $3",
      "  </xsl:otherwise>",
      "</xsl:choose>"
    ],
    "description": "XSL Choose"
  },
  "When": {
    "prefix": "when",
    "body": [
      "<xsl:when test=\"$1\">",
      "  $2",
      "</xsl:when>"
    ],
    "description": "XSL When"
  },
  "If": {
    "prefix": "if",
    "body": [
      "<xsl:if test=\"$1\">",
      "  $2",
      "</xsl:if>"
    ],
    "description": "XSL If"
  },
  "Template": {
    "prefix": "temp",
    "body": [
      "<xsl:template match=\"$1\">",
      "  $2",
      "</xsl:template>"
    ],
    "description": "XSL Template"
  },
  "Value of": {
    "prefix": "val",
    "body": [
      "<xsl:value-of select=\"$1\" />"
    ],
    "description": "XSL Value Of"
  },
  "Otherwise": {
    "prefix": "other",
    "body": [
      "<xsl:otherwise>$1</xsl:otherwise>"
    ],
    "description": "XSL Otherwise"
  },
  "Apply": {
    "prefix": "apply",
    "body": [
      "<xsl:apply-templates match=\"$1\" />"
    ],
    "description": "XSL apply templates"
  },
  "Param": {
    "prefix": "param",
    "body": [
      "<xsl:param name=\"$1\" />"
    ],
    "description": "XSL Parameter"
  },
  "With Param": {
    "prefix": "with",
    "body": [
      "<xsl:with-param name=\"$1\" select=\"$2\"/>"
    ],
    "description": "XSL With Parameter"
  },
  "Attribute": {
    "prefix": "attribute",
    "body": [
      "<xsl:attribute name=\"$1\">",
      "  $2",
      "</xsl:attribute>"
    ],
    "description": "XSL Attribute"
  },
  "Text": {
    "prefix": "text",
    "body": [
      "<xsl:text>$1</xsl:text>"
    ],
    "description": "XSL Text"
  },
  "Copy": {
    "prefix": "copy",
    "body": [
      "<xsl:copy>",
      "  $1",
      "</xsl:copy>"
    ],
    "description": "XSL Copy-of"
  },
  "Copy-Of": {
    "prefix": "copy-of",
    "body": [
      "<xsl:copy-of select=\"$1\">",
      "  $2",
      "</xsl:copy-of>"
    ],
    "description": "XSL Copy-of"
  },
  "For-Each": {
    "prefix": "for-each",
    "body": [
      "<xsl:for-each select=\"$1\">",
      "  $2",
      "</xsl:for-each>"
    ],
    "description": "XSL For-each"
  },
  "Call-Template": {
    "prefix": "call-template",
    "body": [
      "<xsl:call-template name=\"$1\">",
      "  $2",
      "</xsl:call-template>"
    ]
  }
}
